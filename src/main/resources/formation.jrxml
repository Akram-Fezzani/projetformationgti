	<parameter name="id" class="java.lang.Long"/>
	<queryString language="SQL">
		<![CDATA[SELECT
    c.id AS client_id,
    c.cin,
    c.nom AS client_nom,
    c.prenom,
    c.date_ns,
    c.situation_familiale,
    o.id AS compte_id,
    o.numcompte,
    o.date_ouverture,
    o.devise,
    o.client_id AS compte_client_id,
    cr.id AS credit_id,
    cr.montant AS credit_montant,
    cr.nbr_echeance,
    cr.entree_relation,
    cr.date_demande,
    cr.display,
    cr.par,
    cr.observation,
    cr.type,
    cr.unite,
    cr.compte_id AS credit_compte_id,
    cr.status,
    cr.taux,
    cr.echeance,
    cr.montant_echeance
FROM
    Compte o
    LEFT JOIN Client c ON o.client_id = c.id
    LEFT JOIN Credit cr ON o.id = cr.compte_id
WHERE
    cr.id =  $P{id}]]>
	</queryString>
